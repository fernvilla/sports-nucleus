{
  "restartable": "rs",
  "ignore": [".git", "node_modules/**/node_modules"],
  "verbose": true,
  "watch": [
    "server.js",
    "config/**/*.js",
    "bin/www",
    "db/**/*.js",
    "routes/**/*.js",
    "controllers/**/*.js",
    "admin/**/*.js"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "execMap": {
    "js": "node --harmony"
  }
}
