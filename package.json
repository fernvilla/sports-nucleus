{
  "name": "sportsnucleus",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "start": "node ./bin/www",
    "server": "nodemon ./bin/www",
    "client": "cd client && npm run start",
    "dev": "npm-run-all -p server client",
    "fetch:tweets": "node data/fetchTweets",
    "delete:tweets": "node data/deleteTweets",
    "fetch:articles": "node data/fetchArticles",
    "delete:articles": "node data/deleteArticles",
    "fetch": "yarn run fetch:articles && yarn run fetch:tweets",
    "delete": "yarn run delete:articles && yarn run delete:tweets",
    "heroku-postbuild": "cd client && yarn --production=false && yarn run build"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.0.0",
    "html-entities": "^1.3.1",
    "isomorphic-unfetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.3.0",
    "rss-parser": "^3.9.0",
    "sequelize": "^6.3.4",
    "twitter-lite": "^0.14.0"
  },
  "devDependencies": {
    "@types/react-paginate": "^6.2.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "sequelize-cli": "^6.2.0"
  }
}
