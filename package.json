{
  "name": "sportshubla",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "start": "node ./bin/www",
    "server": "nodemon ./bin/www",
    "client": "cd client && npm run start",
    "dev": "npm-run-all -p server client",
    "fetch:tweets": "node dataActions/fetchTweets",
    "delete:tweets": "node dataActions/deleteTweets",
    "fetch:articles": "node dataActions/fetchArticles",
    "delete:articles": "node dataActions/deleteArticles",
    "fetch:videos": "node dataActions/fetchVideos",
    "delete:videos": "node dataActions/deleteVideos",
    "fetch": "yarn run fetch:articles && yarn run fetch:tweets && yarn run fetch:videos",
    "delete": "yarn run delete:articles && yarn run delete:tweets && yarn run delete:videos",
    "heroku-postbuild": "cd client && yarn --production=false && yarn run build"
  },
  "dependencies": {
    "@admin-bro/express": "^3.0.1",
    "@admin-bro/sequelize": "^1.1.1",
    "@sendgrid/mail": "^7.2.6",
    "admin-bro": "^3.2.5",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.1",
    "helmet": "^4.1.1",
    "html-entities": "^1.3.1",
    "isomorphic-unfetch": "^3.1.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.4.1",
    "rss-parser": "^3.9.0",
    "sequelize": "^6.3.5",
    "twitter-lite": "^0.14.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "sequelize-cli": "^6.2.0"
  }
}
