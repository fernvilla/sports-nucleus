{
  "name": "sports-nucleus",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "nodemon",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "fetch:tweets": "node server/data/fetchTweets",
    "delete:tweets": "node server/data/deleteTweets",
    "fetch:articles": "node server/data/fetchArticles",
    "delete:articles": "node server/data/deleteArticles",
    "fetch": "yarn run fetch:articles && yarn run fetch:tweets",
    "delete": "yarn run delete:articles && yarn run delete:tweets"
  },
  "dependencies": {
    "@chakra-ui/core": "^0.8.0",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "cross-env": "^7.0.2",
    "date-fns": "^2.15.0",
    "dotenv": "^8.2.0",
    "emotion-theming": "^10.0.27",
    "express": "^4.17.1",
    "html-entities": "^1.3.1",
    "isomorphic-unfetch": "^3.0.0",
    "morgan": "^1.10.0",
    "next": "latest",
    "pg": "^8.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "rss-parser": "^3.9.0",
    "sass": "^1.26.10",
    "sequelize": "^6.3.3",
    "twitter-lite": "^0.14.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.7",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.26",
    "@types/react": "^16.9.43",
    "@types/react-dom": "16.9.8",
    "nodemon": "^2.0.4",
    "sequelize-cli": "^6.2.0",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "^3.9.7"
  }
}
